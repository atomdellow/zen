
<%- include("headerbar"); -%>
  <div class="spacer" style="height:7px;"></div>
  <!-- carousel background full span-->
  <svg class="bd-placeholder-img card-img-top" style="position: absolute; left: 0px; display: block; height: auto; line-height: 1;" max-width="1000" max-height="400" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Image cap"  focusable="false">
    <rect max-width="1000px" height="400px" fill="#868e96"></rect>
  </svg>
  <!-- carousel container -->
  <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="false" style="position: relative;">
    <!-- carousel indicators -->
    <div class="carousel-indicators">
      <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
      <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
      <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <!-- carousel content -->
    <div class="carousel-inner">
      <!-- item 1 -->
      <div class="carousel-item active">
        <img style="object-fit: cover;" class="bd-placeholder-img bd-placeholder-img-lg d-block w-100" width="800" height="400" aria-label="Placeholder: First slide" preserveAspectRatio="xMidYMid slice" focusable="false" src="/img/bg_slide01.jpg" alt="waterfall slide background">
      </div>
      <!-- item 2 -->
      <div class="carousel-item">
        <img style="object-fit: cover;" class="bd-placeholder-img bd-placeholder-img-lg d-block w-100" width="800" height="400" aria-label="Placeholder: First slide" preserveAspectRatio="xMidYMid slice" focusable="false" src="/img/bg_slide02.jpg" alt="drone city shot slide background">
      </div>
      <!-- item 3 -->
      <div class="carousel-item">
        <img style="object-fit: cover;" class="bd-placeholder-img bd-placeholder-img-lg d-block w-100" width="800" height="400" aria-label="Placeholder: First slide" preserveAspectRatio="xMidYMid slice" focusable="false" src="/img/bg_slide03.jpg" alt="drone city shot slide background">
      </div>
    </div>
    <!-- Directional navigation -->
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
  <div class="spacer" style="height:7px;"></div>
  <!-- card containers -->
  <div class="card-container d-flex justify-content-center">
    <!-- card 1: title and image -->
    <div class="card" style="width: 32rem;">
      <img src="/img/profile_header01.jpg" alt="coding image">
      <!-- card 1: body -->
      <div class="card-body">
        <h5 class="card-title">Profile Updated</h5>
        <p class="card-text">I've created and updated the profile I keep.</p>
        <a href="/about" class="btn btn-primary">Check out my Skills here</a>
      </div>
    </div>
    <!-- card 2: title and image -->
    <div class="card" style="width: 32rem;">
      <iframe width="300" height="200" src="https://www.youtube.com/embed/SGiCBPobRpg" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe><br/>

      <!-- card 2: body -->
      <div class="card-body">
        <h5 class="card-title">Vena VR: Art Gallery</h5>
        <p class="card-text">Back in 2017, I made VR Art Gallery. Check out the reactions here.</p>
        <a href="#" class="btn btn-primary">VR Art Ga</a>
      </div>
    </div>
    <!-- card 3: title and image -->
    <div class="card" style="width: 32rem;">
      <img src="/img/blog_01.jpg" alt="blog stock image" width="300" height="200">
      <!-- card 3: body -->
      <div class="card-body ">
        <h5 class="card-title">Blog Posts</h5>
        <p class="card-text">The site is new, so bear with me. Checkout the informal updates I make on the Blog I constructed.</p>
        <a href="/blog" class="btn btn-primary">Go to Blog</a>
      </div>
    </div>
  </div>

<!-- Content -->

<%- include("partials/footer"); -%>
